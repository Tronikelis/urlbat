    "author": "Tronikelis",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^28.1.3",
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^28.1.1",
        "prettier": "^2.7.1",
        "ts-jest": "^28.0.5",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    }
