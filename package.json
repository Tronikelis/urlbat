{
    "name": "urlbat",
    "version": "1.0.2",
    "description": "A library to join all parts of a url",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "ts-node-dev --respawn src",
        "prettier": "prettier --write src",
        "lint": "eslint . -c ./.eslintrc.json",
        "build": "tsc -p .",
        "prepublishOnly": "npm run build",
