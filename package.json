{
    "name": "urlbat",
    "version": "2.0.0",
    "description": "A library to join all parts of a url",
    "type": "module",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "exports": {
        "import": {
            "default": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "require": {
            "default": "./dist/index.cjs",
            "types": "./dist/index.d.cts"
        }
    },
    "scripts": {
        "prettier": "prettier --write src",
        "lint": "eslint . -c ./.eslintrc.json",
        "build": "tsup",
        "prepublishOnly": "npm run test && npm run build",
        "test": "jest"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Tronikelis/urlbat.git"
    },
    "keywords": [
        "url",
        "uri",
        "build",
        "builder",
        "concat",
        "concatenate",
        "urlcat"
    ],
    "author": "Tronikelis",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "29.5.3",
        "@types/node": "18.17.3",
        "@typescript-eslint/eslint-plugin": "6.2.1",
        "@typescript-eslint/parser": "6.2.1",
        "eslint": "8.46.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-prettier": "5.0.0",
        "jest": "29.6.2",
        "prettier": "3.0.1",
        "typescript": "5.1.6",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "tsup": "7.2.0"
    }
}
